{
  "name": "kqa",
  "version": "1.3.1",
  "private": true,
  "description": "Gerador de dados fictÃ­cios para QA",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:run": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "lint": "eslint src/ --ext .js,.jsx",
    "lint:fix": "eslint src/ --ext .js,.jsx --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "version:patch": "npm version patch && npm run changelog:update",
    "version:minor": "npm version minor && npm run changelog:update",
    "version:major": "npm version major && npm run changelog:update",
    "version:check": "node scripts/version-check.js",
    "changelog:generate": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:update": "npm run changelog:generate && git add CHANGELOG.md",
    "release:patch": "npm run test:unit:run && npm run lint && npm run version:patch && git push --follow-tags",
    "release:minor": "npm run test:unit:run && npm run lint && npm run version:minor && git push --follow-tags",
    "release:major": "npm run test:unit:run && npm run lint && npm run version:major && git push --follow-tags",
    "deploy:check": "node scripts/deploy-version.js"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@faker-js/faker": "^8.4.1",
    "@tiptap/extension-color": "^2.14.0",
    "@tiptap/extension-highlight": "^2.14.0",
    "@tiptap/extension-image": "^2.14.0",
    "@tiptap/extension-task-item": "^2.14.0",
    "@tiptap/extension-task-list": "^2.14.0",
    "@tiptap/extension-text-style": "^2.14.0",
    "@tiptap/extension-underline": "^2.14.0",
    "@tiptap/pm": "^2.14.0",
    "@tiptap/react": "^2.14.0",
    "@tiptap/starter-kit": "^2.14.0",
    "crypto-js": "^4.2.0",
    "graphql": "^16.11.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-rnd": "^10.5.2",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@vitejs/plugin-react": "^4.2.1",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "semver": "^7.5.4",
    "vite": "^4.0.0",
    "vitest": "^1.0.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
